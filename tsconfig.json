{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "esnext",
    "module": "es2022",
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "types": ["vite/client"]
  }
}
